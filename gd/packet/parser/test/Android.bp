genrule {
    name: "BluetoothPacketParserTestPacketPdlGen_h",
    tools: [
        "bluetooth_packetgen",
    ],
    cmd: "mkdir -p $(genDir)/packet/parser/test/; $(location bluetooth_packetgen) < $(in) > $(genDir)/packet/parser/test/generated_packets.h 2>/dev/null",
    srcs: [
        "test_packet.pdl",
    ],
    out: [
        "packet/parser/test/generated_packets.h",
    ],
}

filegroup {
    name: "BluetoothPacketParserTestPacketTestSources",
    srcs: [
        "generated_packet_test.cc",
    ],
}
